<div class="symbaroum chat combat">
    <div class="foreground">
        <div class="introImg" style="background-image: url('{{introImg}}')">
            <div class="introTxt">{{removeMarkup introText}}</div>
            {{#if hasTarget}}
            <div class="introImg" style="background-image: url('{{targetData.actor.img}}')"></div>
        </div>
        <div class="targetText">{{removeMarkup targetText}}</div>
        {{/if}}
        <div class="subText">{{removeMarkup subText}}</div>            
        <img src="{{subImg}}"/>
        {{#if hasRoll}}
        <div class="finalTxt">
            {{#if resistRoll}}
                <p>{{resistRollText}}</p>
            {{/if}}
            <p  data-item-id="{{id}}">{{removeMarkup rollString}}</p>
        </div>
            {{/if}}
        {{#each rollData as |rollDataElement|}}
        <div class="finalTxt">
            <div class="tooltip">
                <p>{{removeMarkup rollDataElement.rollResult}}</p>
                <span class="tooltiptext">{{rollDataElement.toolTip}}</span>
            </div>
        </div>
        <h4>{{rollDataElement.resultText}}</h4>
        {{#if rollDataElement.hasDamage}}
        {{#if hasDmgMod}}
        <p>{{damageRollMod}}</p>
        {{/if}}
        <div class="finalTxt">{{rollDataElement.dmgFormula}}
        </div>
        <div class="finalTxt">
            <div class="tooltip">
                <p>{{rollDataElement.damageText}}</p>
                <span class="tooltiptext">{{rollDataElement.damageTooltip}}</span>
            </div>
        </div>
        {{/if}}
        {{/each}}
        {{#if hasDamage}}
        <div class="finalTxt">
            <p>{{damageFinalText}}</p>
        </div>
        {{/if}}
        {{#if hasCorruption}}
        <p>{{removeMarkup corruptionText}}</p>
        {{/if}}
        {{#if printPoison}}
            <h4>{{removeMarkup poisonChatIntro}}</h4>
            {{#if resistRoll}}
            <div class="finalTxt">{{removeMarkup poisonResistRollText}}</div>
            {{/if}}
            <div class="finalTxt">{{removeMarkup poisonRollString}}</div>
            <div class="finalTxt">
                <div class="tooltip">
                    <p>{{removeMarkup poisonRollResultString}}</p>
                    <span class="tooltiptext">{{poisonToolTip}}</span>
                </div>
            </div>
        <h4>{{removeMarkup poisonChatResult}}</h4>
        {{/if}}
        {{#if printCorruption}}
        <div class="finalTxt">
            <div class="tooltip">
                <p>{{corruptionChatResult}}</p>
                <span class="tooltiptext">{{corruptionTooltip}}</span>
            </div>
        </div>
        {{/if}}
        {{#if printBleed}}
        <div class="finalTxt">{{removeMarkup bleedChat}}</div>
        {{/if}}
        {{#if printFlaming}}
        <div class="finalTxt">{{removeMarkup flamingChat}}</div>
        {{/if}}
    </div>
</div>